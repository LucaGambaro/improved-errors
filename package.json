{
  "name": "improved-errors",
  "version": "6.1.0",
  "main": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "scripts": {
    "ts-compile": "rm -rf ./dist && tsc --project ./tsconfig.json",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write \"./src/**/*.ts\" --loglevel warn",
    "check": "npm run ts-compile && npm run lint && npm run format",
    "prepublishOnly": "npm run ts-compile",
    "preversion": "npm run check && npm run atest",
    "pretest": "npm run check"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=9.8.x <11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucaGambaro/improved-errors.git" 
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.5.2"
  },
  "author": {
    "name": "Luca Gambaro"
  },
  "license": "MIT"
}
